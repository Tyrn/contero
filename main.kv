BoxLayout:
    orientation: "vertical"

    #MDToolbar:
    #    title: "Example Tabs"

    MDTabs:
        id: android_tabs
        on_tab_switch: app.on_tab_switch(*args)


<TabMain>:
    ScrollView:
        id: scroll_view
        MDList:
            id: container


<TabList>:
    MDIconButton:
        id: icon
        user_font_size: "48sp"
        pos_hint: {"center_x": .5, "center_y": .5}


<TabDetails>:
    MDIconButton:
        id: icon
        user_font_size: "48sp"
        pos_hint: {"center_x": .5, "center_y": .5}

